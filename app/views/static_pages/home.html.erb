<div class="culmn">
  <section id="hello" class="home bg-mega">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="main_home text-center">
          <div class="home_text">
            <h4 class="text-white"><em><%= t ".quotations" %></em></h4>
            <h5 class="text-white"><em><%= t ".author" %></em></h5>
          </div>
        </div>
      </div>
    </div>
  </section> 
  <section id="feature" class="feature p-top-100">
    <div class="container">
      <div class="row">
        <div class="main_feature">
          <div class="col-md-6 m-top-50">
            <div class="head_title">
              <h2><%= link_to t(".yogis_read"), posts_path %></h2>
              <h5><em><%= t ".description" %></em></h5>
              <div class="separator_left"></div>
            </div>
            <div class="feature_content wow fadeIn m-top-40">
              
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="feature_photo wow fadeIn sm-m-top-40">
            <div class="photo_border"></div>
            <div class="feature_img">
              <%= link_to image_tag("yogis_read.jpg"), "#" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section> <!--End off About section -->
  <section id="gallery" class="gallery margin-top-120 bg-grey">
    <div class="container">
      <div class="row">
        <div class="main-gallery roomy-80">
          <div class="col-md-12">
            <div class="head_title text-left sm-text-center wow fadeInDown">
              <h2><%= t ".yogis_watch" %></h2>
              <h5><em><%= t ".des_watch" %></em></h5>
              <div class="separator_left"></div>
            </div>
          </div>
          <div class="col-md-12">
            
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- scroll up-->
  <div class="scrollup">
    <a href="#"><i class="fa fa-chevron-up"></i></a>
  </div><!-- End off scroll up -->
</div>
